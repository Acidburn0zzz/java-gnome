(define-object CommandLine
  (in-module "Glib")
  (parent "GObject")
  (c-name "GApplicationCommandLine")
)

(define-method get_arguments
  (of-object "GApplicationCommandLine")
  (c-name "g_application_command_line_get_arguments")
  (return-type "gchar**")
  (parameters
    '("int*" "argc")
  )
)

