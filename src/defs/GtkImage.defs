(define-object Image
  (in-module "Gtk")
  (parent "GtkMisc")
  (c-name "GtkImage")
  (gtype-id "GTK_TYPE_IMAGE")
)

(define-function gtk_image_new
  (is-constructor-of "GtkImage")
  (c-name "gtk_image_new")
  (caller-owns-return #t)
  (return-type "GtkWidget*")
)

(define-method clear
  (of-object "GtkImage")
  (c-name "gtk_image_clear")
  (return-type "none")
)

