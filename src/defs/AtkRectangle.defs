(define-boxed Rectangle
  (in-module "Atk")
  (c-name "AtkRectangle")
  (gtype-id "ATK_TYPE_RECTANGLE")
  (fields
    '("gint" "x")
    '("gint" "y")
    '("gint" "width")
    '("gint" "height")
  )
)

(define-function rectangle_new
  (is-constructor-of "AtkRectangle")
  (c-name "atk_rectangle_new")
  (caller-owns-return #t)
  (return-type "AtkRectangle")
)

