;; Test for methods that take enum parameters

(define-object Button
  (in-module "Gtk")
  (parent "GtkBin")
  (c-name "GtkButton")
  (gtype-id "GTK_TYPE_BUTTON")
)

(define-method set_enum
  (of-object "GtkButton")
  (c-name "gtk_button_set_enum")
  (return-type "none")
  (parameters
    '("GtkEnum" "enum")
  )
)
