;; Test for methods that return an Object value

(define-object Button
  (in-module "Gtk")
  (parent "GtkBin")
  (c-name "GtkButton")
  (gtype-id "GTK_TYPE_BUTTON")
)

(define-method get_image
  (of-object "GtkButton")
  (c-name "gtk_button_get_image")
  (return-type "GtkWidget*")
  (parameters
    '("GError**" "error")
  )
)

(define-method set_int
  (of-object "GtkButton")
  (c-name "gtk_button_set_int")
  (return-type "none")
  (parameters
    '("gint" "i")
    '("GError**" "error")
  )
)
