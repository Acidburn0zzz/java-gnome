;; Test for methods that take ouput native parameters

(define-object Button
  (in-module "Gtk")
  (parent "GtkBin")
  (c-name "GtkButton")
  (gtype-id "GTK_TYPE_BUTTON")
)

(define-method get_widget
  (of-object "GtkButton")
  (c-name "gtk_button_get_widgets")
  (return-type "none")
  (parameters
    '("GtkWidget**" "widgets")
  )
)
