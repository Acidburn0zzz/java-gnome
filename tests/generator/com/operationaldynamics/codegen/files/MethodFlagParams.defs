;; Test for methods that take flags parameters

(define-object Button
  (in-module "Gtk")
  (parent "GtkBin")
  (c-name "GtkButton")
  (gtype-id "GTK_TYPE_BUTTON")
)

(define-method set_flags
  (of-object "GtkButton")
  (c-name "gtk_button_set_flags")
  (return-type "none")
  (parameters
    '("GtkFlags" "flags")
  )
)
