;; Test for methods that take enum parameters

(define-object Button
  (in-module "Gtk")
  (parent "GtkBin")
  (c-name "GtkButton")
  (gtype-id "GTK_TYPE_BUTTON")
)

(define-method set_boxed
  (of-object "GtkButton")
  (c-name "gtk_button_set_boxed")
  (return-type "none")
  (parameters
    '("GtkBoxed*" "boxed")
  )
)

(define-method set_boxed_nullable
  (of-object "GtkButton")
  (c-name "gtk_button_set_boxed_nullable")
  (return-type "none")
  (parameters
    '("GtkBoxed*" "boxed" (null-ok))
  )
)
